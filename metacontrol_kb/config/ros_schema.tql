define

package sub attribute,
    value string;

executable sub attribute,
    value string;

ROSNode sub Component,
    owns package,
    owns executable;
   
ros-parameter-type sub attribute,
    value string,
    regex "^(bool|bool_array|double|double_array|integer|integer_array|string|string_array)$";   

ROSParam sub ComponentParameter,
    owns ros-parameter-type;
